{
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017,
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "rules": {
    "no-undef": "error",
    "no-unused-vars": "error",
    "react/prop-types": "off"
  },
  "env": {
    "browser": true
  }
}
